{
  "seo": {
    "type": "website",
    "title": "We.digi",
    "jsonLDs": [],
    "noIndexing": false,
    "description": "Consultoria de e-commerce especialista em VTEX, Shopify e Uappi. Soluções ágeis, personalizadas e com foco em performance. Conheça a metodologia we.digi.",
    "titleTemplate": "Consultoria Especializada em E-commerce | we.digi​",
    "descriptionTemplate": "%s ",
    "favicon": "https://assets.decocache.com/we-digi/3356c270-a5ad-4a8b-a0b0-1c8f66a1b951/favicon-16x16.png",
    "image": "https://assets.decocache.com/we-digi/4b5e1ac5-f071-423f-8ea0-f1947919a68f/banner-wedigi.jpg"
  },
  "global": [
    {
      "__resolveType": "htmx/sections/htmx.tsx"
    },
    {
      "__resolveType": "website/sections/Analytics/Analytics.tsx",
      "trackingIds": [
        "GTM-WW3F56G"
      ],
      "src": "https://metrics.wedigi.com.br"
    }
  ],
  "routes": [
    {
      "__resolveType": "website/loaders/pages.ts"
    },
    {
      "__resolveType": "website/loaders/redirects.ts"
    },
    [
      {
        "handler": {
          "value": {
            "__resolveType": "website/handlers/sitemap.ts",
            "excludePaths": []
          }
        },
        "pathTemplate": "/sitemap/deco.xml",
        "isHref": false
      },
      {
        "handler": {
          "value": {
            "__resolveType": "site/handlers/sitemapblog.ts",
            "posts": {
              "__resolveType": "blog/loaders/BlogpostList.ts",
              "postSlugs": [],
              "sortBy": "date_desc",
              "count": 10000
            }
          }
        },
        "pathTemplate": "/sitemap/blog.xml"
      },
      {
        "handler": {
          "value": {
            "__resolveType": "site/handlers/sitemapIndex.ts",
            "sitemaps": [
              "/sitemap/deco.xml",
              "/sitemap/blog.xml"
            ]
          }
        },
        "pathTemplate": "/sitemap.xml"
      }
    ]
  ],
  "caching": {
    "directives": []
  },
  "__resolveType": "site/apps/site.ts",
  "firstByteThresholdMS": false,
  "theme": {
    "__resolveType": "We.digi - Dark"
  },
  "abTesting": {
    "replaces": [],
    "includeScriptsToHead": {
      "includes": []
    },
    "includeScriptsToBody": {
      "includes": []
    }
  },
  "flavor": {
    "framework": "fresh"
  }
}